{
  "apiVersion": "1.0.0",
  "swaggerVersion": "2.0",
  "basePath": "http://localhost:3002",
  "resourcePath": "/users",
  "produces": ["application/json", "text/plain"],
  "apis": [
    {
      "path": "/users/login",
      "operations": [
        {
          "method": "POST",
          "summary": "用户登录",
          "type": "LoginSuccess",
          "notes": "",
          "nickname": "login",
          "produces": ["application/json", "text/plain"],
          "consumes": ["application/x-www-form-urlencoded", "application/json"],
          "parameters": [
            {
              "name": "email",
              "type": "string",
              "paramType": "form",
              "required": true,
              "description": "用户邮箱"
            },
            {
              "name": "password",
              "type": "string",
              "paramType": "form",
              "required": true,
              "description": "用户密码"
            },
            {
              "name": "X-Parse-Application-Id",
              "type": "string",
              "paramType": "header",
              "required": true,
              "description": "app id"
            },
            {
              "name": "X-Parse-REST-API-Key",
              "type": "string",
              "paramType": "header",
              "required": true,
              "description": "api key"
            },
            {
              "name": "X-Device-Id",
              "type": "string",
              "paramType": "header",
              "required": true,
              "description": "设备id"
            },
            {
              "name": "X-Device-Type",
              "type": "string",
              "paramType": "header",
              "required": true,
              "description": "设备型号"
            },
            {
              "name": "X-Platform",
              "type": "string",
              "paramType": "header",
              "required": true,
              "description": "平台系统"
            },
            {
              "name": "X-Version",
              "type": "string",
              "paramType": "header",
              "required": true,
              "description": "平台版本"
            }
          ],
          "responseMessages": [
            {
              "code": 400,
              "message": "邮箱格式不正确"
            },
            {
              "code": 400,
              "message": "缺少邮箱或密码"
            },
            {
              "code": 401,
              "message": "邮箱或密码错误"
            }
          ]
        }
      ]
    }
  ],
  "models": {
    "LoginSuccess": {
      "id": "LoginSuccess",
      "properties": {
        "token": {
          "type": "string",
          "description": "access token, 有效期7天",
          "required": true
        }
      }
    }
  }
}